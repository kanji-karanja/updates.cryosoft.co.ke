<?xml version="1.0"?>
<configuration>
<system.webServer>
        <rewrite>
             <rule name="rule 1V" stopProcessing="true">
	            <match url="^api/([0-9a-zA-Z_-]*)$"  ignoreCase="true" />
	            <action type="Rewrite" url="/api.php?app_name={R:1}"  />
              </rule>
        </rewrite>
  <system.web>
    <compilation debug="true"/>
  </system.web>
</configuration>
